{
    "name": "mcesar/laravel-survey",
    "description": "Survey package for Laravel 5.5 and up",
    "keywords": [
        "laravel",
        "survey",
        "question",
        "answer"
    ],
    "homepage": "https://github.com/milo526/laravel-survey",
    "license": "MIT",
    "authors": [
        {
            "name": "Milo Cesar",
            "email": "me@mcesar.eu"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "MCesar\\Survey\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MCesar\\Survey\\Test\\": "tests"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MCesar\\Survey\\SurveyServiceProvider"
            ]
        }
    }
}
